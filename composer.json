{
    "name": "helmich/graphizer",
    "description": "Store PHP syntax trees in Neo4j graph structures",
    "license": "LGPL",
    "authors": [
        {
            "name": "Martin Helmich",
            "email": "kontakt@martin-helmich.de"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/martin-helmich/neo4jphp"
        }
    ],
    "require": {
        "php": ">= 5.5",
        "nikic/php-parser": "dev-master",
        "phpdocumentor/reflection-docblock": "*",
        "symfony/console": "*",
        "justinrainbow/json-schema": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phing/phing": "*",
        "fzaninotto/faker": "~1.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Helmich\\Graphizer\\Tests\\": "tests/",
            "Helmich\\Graphizer\\":"src/"
        }
    }
}
